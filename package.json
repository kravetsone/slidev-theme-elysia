{
    "name": "slidev-theme-elysia",
    "version": "0.0.2",
    "description": "Elysia theme for slidev",
    "type": "module",
    "keywords": [
        "slidev-theme",
        "slidev",
        "elysia"
    ],
    "engines": {
        "node": ">=18.0.0",
        "slidev": ">=0.19.3"
    },
    "scripts": {
        "build": "slidev build ./example/example.md",
        "dev": "slidev ./example/example.md --open",
        "export": "slidev export ./example/example.md",
        "screenshot": "slidev export ./example/example.md --format png"
    },
    "dependencies": {
        "@slidev/types": "0.48.3"
    },
    "devDependencies": {
        "@elysiajs/eden": "^1.0.11",
        "@slidev/cli": "0.48.3",
        "elysia": "^1.0.13",
        "playwright-chromium": "^1.43.1"
    },
    "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
    "slidev": {
        "colorSchema": "dark",
        "highlighter": "shiki",
        "defaults": {
            "fonts": {
                "sans": "Inter",
                "mono": "Consolas"
            }
        }
    }
}
