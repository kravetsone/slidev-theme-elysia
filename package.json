{
    "name": "slidev-theme-elysia",
    "version": "0.0.0",
    "type": "module",
    "keywords": [
        "slidev-theme",
        "slidev"
    ],
    "engines": {
        "node": ">=18.0.0",
        "slidev": ">=0.19.3"
    },
    "scripts": {
        "build": "slidev build example.md",
        "dev": "slidev example.md --open",
        "export": "slidev export example.md",
        "screenshot": "slidev export example.md --format png"
    },
    "dependencies": {
        "@slidev/types": "^0.48.0-beta.0"
    },
    "devDependencies": {
        "@slidev/cli": "^0.48.0-beta.0"
    },
    "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
    "slidev": {
        "colorSchema": "dark",
        "highlighter": "shiki",
        "defaults": {
            "fonts": {
                "sans": "Inter",
                "mono": "Consolas"
            }
        }
    }
}
